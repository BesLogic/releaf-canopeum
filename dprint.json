{
  // https://dprint.dev/config/#extending-a-different-configuration-file
  "extends": "https://raw.githubusercontent.com/BesLogic/beslogic-dprint-configuration/main/dprint.json",
  "excludes": [
    "**/node_modules",
    "**/*-lock.json",
    "**/.yalc",
    "**/coverage", // created on install by nx
    "**/build",
    "**/cache",
    "**/dist",
    "**/tmp",
    "**/obj",
    // capacitor folders
    "**/*/android/app",
    "**/*/ios/App",
    "**/*/services/api.ts"
  ]
}
